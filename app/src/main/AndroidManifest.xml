<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.donateapplication">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DONATEAPPLICATION">
        <activity android:name=".ClothesDonateFormActivity"></activity>
        <activity android:name=".NGO_HomeActivity" />
        <activity android:name=".DataDisplayActivity" />
        <activity android:name=".PicActivity" />
        <activity android:name=".FeedbackActivity" />
        <activity android:name=".MyDonationActivity" />
        <activity android:name=".FormActivity" />
        <activity android:name=".DonateActivity" />
        <activity android:name=".NGO_Interface_Activity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".HomeScreenActivity" />
        <activity android:name=".SignUPActivity" />
        <activity android:name=".SignINActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>